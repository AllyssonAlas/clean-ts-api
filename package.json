    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-promise": "^5.2.0",
    "faker": "^4.1.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^12.3.7",
    "mockdate": "^3.0.5",
